{
  "name": "Password Reset Request",
  "description": "Simple password reset workflow with security verification",
  "category": "IT Support",
  "tags": ["it", "security", "password", "simple"],
  "complexity": "simple",
  "estimatedSetupTime": "3 minutes",
  "estimatedDuration": "15-30 minutes",
  "icon": "lock",
  "useCases": ["Forgotten password", "Account lockout", "Security reset"],
  "prerequisites": [],
  "integrationRequirements": ["Active Directory", "Email"],
  "nodes": [
    {"id": "start-1", "type": "start", "position": {"x": 100, "y": 200}, "data": {"label": "Start Request"}},
    {"id": "form-1", "type": "form", "position": {"x": 300, "y": 200}, "data": {"label": "Reset Form", "formFields": [{"id": "username", "label": "Username", "type": "text", "required": true}, {"id": "email", "label": "Email", "type": "email", "required": true}, {"id": "security_question", "label": "Security Answer", "type": "text", "required": true}]}},
    {"id": "task-1", "type": "task", "position": {"x": 500, "y": 200}, "data": {"label": "Verify Identity", "description": "IT verifies user identity", "assignee": "it-helpdesk@company.com"}},
    {"id": "decision-1", "type": "decision", "position": {"x": 700, "y": 200}, "data": {"label": "Verified?", "conditions": [{"condition": "${task-1.verified} == true", "label": "Yes", "targetId": "task-2"}, {"condition": "${task-1.verified} == false", "label": "No", "targetId": "end-2"}]}},
    {"id": "task-2", "type": "task", "position": {"x": 900, "y": 100}, "data": {"label": "Reset Password", "description": "Generate temporary password", "assignee": "it-helpdesk@company.com"}},
    {"id": "action-1", "type": "action", "position": {"x": 1100, "y": 100}, "data": {"label": "Send Password", "actionType": "email", "config": {"to": "${form-1.email}", "subject": "Password Reset"}}},
    {"id": "end-1", "type": "end", "position": {"x": 1300, "y": 100}, "data": {"label": "Complete"}},
    {"id": "end-2", "type": "end", "position": {"x": 900, "y": 300}, "data": {"label": "Verification Failed"}}
  ],
  "edges": [{"id": "e1", "source": "start-1", "target": "form-1"}, {"id": "e2", "source": "form-1", "target": "task-1"}, {"id": "e3", "source": "task-1", "target": "decision-1"}, {"id": "e4", "source": "decision-1", "target": "task-2"}, {"id": "e5", "source": "decision-1", "target": "end-2"}, {"id": "e6", "source": "task-2", "target": "action-1"}, {"id": "e7", "source": "action-1", "target": "end-1"}]
}