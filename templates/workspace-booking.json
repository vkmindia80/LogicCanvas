{
  "name": "Workspace Booking",
  "description": "Book desk, meeting room, or workspace for hybrid work",
  "category": "Operations",
  "tags": ["operations", "booking", "workspace", "hybrid", "simple"],
  "complexity": "simple",
  "estimatedSetupTime": "3 minutes",
  "estimatedDuration": "5 minutes",
  "icon": "calendar",
  "useCases": ["Desk booking", "Meeting room booking", "Hot desk reservation"],
  "prerequisites": [],
  "integrationRequirements": [],
  "nodes": [
    {
      "id": "start-1",
      "type": "start",
      "position": {"x": 100, "y": 200},
      "data": {
        "label": "Start Booking"
      }
    },
    {
      "id": "form-1",
      "type": "form",
      "position": {"x": 300, "y": 200},
      "data": {
        "label": "Booking Form",
        "formFields": [
          {"id": "employee_name", "label": "Employee Name", "type": "text", "required": true},
          {"id": "workspace_type", "label": "Workspace Type", "type": "dropdown", "required": true, "options": ["Desk", "Meeting Room", "Private Office", "Phone Booth"]},
          {"id": "date", "label": "Date", "type": "date", "required": true},
          {"id": "time_slot", "label": "Time Slot", "type": "dropdown", "required": true, "options": ["Full Day", "Morning (9-1)", "Afternoon (1-5)"]},
          {"id": "notes", "label": "Special Requirements", "type": "textarea", "required": false}
        ]
      }
    },
    {
      "id": "task-1",
      "type": "task",
      "position": {"x": 500, "y": 200},
      "data": {
        "label": "Check Availability",
        "description": "Verify workspace is available",
        "taskType": "automated"
      }
    },
    {
      "id": "decision-1",
      "type": "decision",
      "position": {"x": 700, "y": 200},
      "data": {
        "label": "Available?",
        "conditions": [
          {"condition": "${task-1.available} == true", "label": "Yes", "targetId": "task-2"},
          {"condition": "${task-1.available} == false", "label": "No", "targetId": "end-2"}
        ]
      }
    },
    {
      "id": "task-2",
      "type": "task",
      "position": {"x": 900, "y": 100},
      "data": {
        "label": "Confirm Booking",
        "description": "Reserve workspace and send confirmation",
        "taskType": "automated"
      }
    },
    {
      "id": "end-1",
      "type": "end",
      "position": {"x": 1100, "y": 100},
      "data": {
        "label": "Booked",
        "description": "Workspace reserved successfully"
      }
    },
    {
      "id": "end-2",
      "type": "end",
      "position": {"x": 900, "y": 300},
      "data": {
        "label": "Unavailable",
        "description": "Workspace not available"
      }
    }
  ],
  "edges": [
    {"id": "e1", "source": "start-1", "target": "form-1"},
    {"id": "e2", "source": "form-1", "target": "task-1"},
    {"id": "e3", "source": "task-1", "target": "decision-1"},
    {"id": "e4", "source": "decision-1", "target": "task-2"},
    {"id": "e5", "source": "decision-1", "target": "end-2"},
    {"id": "e6", "source": "task-2", "target": "end-1"}
  ]
}