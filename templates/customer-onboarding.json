{
  "id": "customer-onboarding-template",
  "name": "Customer Onboarding Workflow",
  "description": "Complete customer onboarding process with welcome email, account setup, and first purchase incentive",
  "category": "sales",
  "tags": ["customer", "onboarding", "sales", "crm"],
  "complexity": "medium",
  "setup_time": "~10 minutes",
  "node_count": 8,
  "nodes": [
    {
      "id": "start-1",
      "type": "start",
      "position": {"x": 100, "y": 100},
      "data": {"label": "Customer Signup", "type": "start"}
    },
    {
      "id": "task-1",
      "type": "task",
      "position": {"x": 100, "y": 200},
      "data": {
        "label": "Send Welcome Email",
        "type": "task",
        "description": "Send personalized welcome email to new customer",
        "assignedTo": "sales@company.com"
      }
    },
    {
      "id": "form-1",
      "type": "form",
      "position": {"x": 100, "y": 300},
      "data": {
        "label": "Profile Setup Form",
        "type": "form",
        "description": "Customer completes profile information"
      }
    },
    {
      "id": "action-1",
      "type": "action",
      "position": {"x": 100, "y": 400},
      "data": {
        "label": "Create CRM Record",
        "type": "action",
        "actionType": "http",
        "url": "https://api.crm.com/contacts",
        "method": "POST"
      }
    },
    {
      "id": "decision-1",
      "type": "decision",
      "position": {"x": 100, "y": 500},
      "data": {
        "label": "First Purchase?",
        "type": "decision",
        "condition": "${first_purchase} == true"
      }
    },
    {
      "id": "task-2",
      "type": "task",
      "position": {"x": 300, "y": 600},
      "data": {
        "label": "Send Discount Code",
        "type": "task",
        "description": "Send 10% discount for first purchase"
      }
    },
    {
      "id": "task-3",
      "type": "task",
      "position": {"x": 100, "y": 700},
      "data": {
        "label": "Schedule Follow-up",
        "type": "task",
        "description": "Schedule follow-up email in 7 days"
      }
    },
    {
      "id": "end-1",
      "type": "end",
      "position": {"x": 100, "y": 800},
      "data": {"label": "Onboarding Complete", "type": "end"}
    }
  ],
  "edges": [
    {"id": "e1", "source": "start-1", "target": "task-1"},
    {"id": "e2", "source": "task-1", "target": "form-1"},
    {"id": "e3", "source": "form-1", "target": "action-1"},
    {"id": "e4", "source": "action-1", "target": "decision-1"},
    {"id": "e5", "source": "decision-1", "target": "task-2", "label": "yes"},
    {"id": "e6", "source": "decision-1", "target": "task-3", "label": "no"},
    {"id": "e7", "source": "task-2", "target": "task-3"},
    {"id": "e8", "source": "task-3", "target": "end-1"}
  ]
}