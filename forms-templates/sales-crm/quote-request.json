{
  "id": "quote-request",
  "name": "Quote Request Form",
  "description": "Request pricing quotes for products and services",
  "category": "sales-crm",
  "icon": "Calculator",
  "version": 1,
  "tags": ["sales", "crm", "quote", "pricing"],
  "fields": [
    {
      "id": "requester_name",
      "type": "text",
      "label": "Requester Name",
      "required": true
    },
    {
      "id": "company_name",
      "type": "text",
      "label": "Company Name",
      "required": true
    },
    {
      "id": "email",
      "type": "email",
      "label": "Email Address",
      "required": true
    },
    {
      "id": "phone",
      "type": "phone",
      "label": "Phone Number",
      "required": true
    },
    {
      "id": "products_services",
      "type": "checkbox",
      "label": "Products/Services Interested In",
      "required": true,
      "options": ["Product A", "Product B", "Product C", "Professional Services", "Implementation", "Training", "Support"]
    },
    {
      "id": "quantity",
      "type": "number",
      "label": "Quantity/Number of Users",
      "required": true,
      "validation": {
        "min": 1
      }
    },
    {
      "id": "contract_length",
      "type": "radio",
      "label": "Preferred Contract Length",
      "required": false,
      "options": ["Monthly", "Annual", "Multi-year", "Not Sure"]
    },
    {
      "id": "budget_range",
      "type": "select",
      "label": "Budget Range",
      "required": false,
      "options": ["Under $10k", "$10k-$50k", "$50k-$100k", "$100k-$500k", "$500k+", "Not Sure"]
    },
    {
      "id": "purchase_timeline",
      "type": "radio",
      "label": "Purchase Timeline",
      "required": true,
      "options": ["Immediate (0-30 days)", "Short-term (1-3 months)", "Long-term (3-12 months)", "Just Researching"]
    },
    {
      "id": "special_requirements",
      "type": "textarea",
      "label": "Special Requirements or Customizations",
      "required": false,
      "placeholder": "Any specific needs or customizations?"
    },
    {
      "id": "additional_info",
      "type": "textarea",
      "label": "Additional Information",
      "required": false
    }
  ]
}